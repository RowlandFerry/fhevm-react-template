  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "hardhat": "^2.26.4",
    "dotenv": "^17.2.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.1"
